<style>
*{
	box-sizing: border-box;
}
html{
	height:100%;
	overflow:auto;
}
::-webkit-scrollbar {
    display:none;
}
body{
	height: 100%;
	margin: 0;
	padding: 0;
	line-height: 1.42857143;
	background-color: #f1f3f3;
	font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
	font-size: 14px;
}
.alert-wrappernb{
	position: absolute;
	width: 100%;
}
.alertnb {
	display: inline-grid;
	z-index: 10;
	position: relative;
	width: 100%;
	height: 0px;
	background-color: #f44336;
	overflow: hidden;
	color: white;
	transition: 0.5s;
}
.d1{
	overflow: hidden;
	background-color: #222D3C;
	position: fixed;
	height: 60px;
	z-index:1;
	width: 100%;
	display: block;
    padding: 0px 15px;
}
#div1-wrapper{
	height: 60px;
}
.d1 a{
	float: left;
	color: #f2f2f2;
	text-align: center;
	padding-right: 50px;
	text-decoration: none;
	font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
	font-size: 20px;
	font-weight: bold;
}
.d1{
	padding-top: 15px;
	box-sizing: border-box;
}
.d1 span{
	color: white;
}
.d2{
	height: 100%;
	width: 50px;;
	position: fixed;
	z-index: 1;
	top: 60px;
	left: 0;
	background-color: #000000;
	overflow: auto;
	transition: 0.5s;
}
#div2-wrapper{
	position: absolute;
	display: inline-block;
}
.d2 a{	
	display: block; 
	padding: 11px 4px 11px 15px;
	text-decoration: none;
	color: #999999;
	transition: 0.1s;
	font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
	font-size: 15px;
	white-space: nowrap;
	cursor: pointer;
}
.fa{
	font-size: 21px;
	padding-right: 10px;
}
.d2 a:hover:not(.active){
	background-color: #111; 
	border-left: 2px solid red;
	color: white;
}
.active{
	background-color: #337AB7; 
}
.active:hover{
	border-left: 2px solid red;
}
.d2 img{
	width: 60px;
	height: 60px;
}
.act{
	color: white;
}
.sadmin{
	float: right;
	padding: .2em .6em .3em;
	font-size: 75%;
	margin-top: 7px;
    font-weight: 700;
    line-height: 1;
	border-radius: 4px;
}
.img1{
	cursor: pointer;
    width: 40px;
    margin-left: 30px;
    margin-right: 10px;
    float: right;
    border-radius: 50%;
}
footer {
    position: fixed;
    bottom: 0;
    width: 100%;
	z-index:20;
}
#div6-wrapper,#divman6-wrapper{
	position: fixed;
	top: 0;
	left: 0;
	z-index:20;
	width:100%;
    display:inline-block;
}
.animdiv{
	position:relative;
	width:0%;
	height:0;
	overflow: hidden;
	transition-duration: 0.4s;
    	transition-timing-function: cubic-bezier(0.36, 0.55, 0.19, 1);
	transition-property: width, height; 
}
#div3{
	background-color: #414F66;
	opacity:0.95;
}
#div6,#divman6{
	background-color: rgba(112,128,144,0.2);	
    display:inline-block;
}
@media (min-width : 0px) {
    #divxx9,#divman9 { display:inline-block;width: 83.33%; }
}    
@media (min-width : 768px) {
    #divxx9,#divman9 { display:inline-block;width: 345px; }
}
@media (min-width : 992px) {
    #divxx9,#divman9 { display:inline-block;width: 293.33px; }
}
#divxx9,#divman9{
    position: absolute;
    top: 50%;
    left: 50%;
    z-index:20;
    transform: translate(-50%, -50%);
	margin: auto;
	overflow: hidden;
	float: left;
	border-radius: 5px;
	box-shadow: 0 2px 6px rgba(0,0,0,0.2);
	padding: 15px 15px;
	margin-left:auto;
	margin-right:auto;
	line-height: 20px;
}
#b4,#b3,#b4man,#b3man{
	text-rendering: auto;
	height: 32px;
	color:white;
	font-size: 14px;
	font-weight: 900;
	margin:0px 2px 4px;
	padding: 6px 12px;
	text-align: center;
	cursor: pointer;
	border: 0;
	border-radius: 4px;
	float: right;
}
#b3,#b3man{
	background-color:#5cb85c;
}
#b4,#b4man{
	background-color:#d9534f;
}
#b1,#b2{
	background-color:#303F53;
	color:white;
	text-shadow:none;
	padding:10px;
	min-width:100px;
	font-size:16px;
	cursor:pointer;
	border:0;
	border-radius:2px;
	margin-bottom: 4px;
    margin-left: 2px;
    margin-right: 2px;
}
.d4{
	animation: shake 0.82s infinite;
	transform: translate3d(0, 0, 0);
	backface-visibility: hidden;
	animation-play-state: paused;
}
@keyframes shake{
	0%,%,55%,85% { transform: translate(1px, 0px); }
	10%,30%,60%,90% { transform: translate(-1px, -0px); }
	20%,35%,65%,95% { transform: translate(-4px, 0px); }
	30%,40%,70%,100% { transform: translate(4px, 0px); }
	40%,45%,75% { transform: translate(-8px, 0px); }
	50%,50%,80% { transform: translate(8px, 0px); 
}
#b1,#b2{
	animation-play-state: paused;
}
</style>
