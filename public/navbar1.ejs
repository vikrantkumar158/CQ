<script>
function funcNav()
{
	var div2=document.getElementById('div2');
	if(div2.style.width=="250px")
	{
		div2.style.width="50px";
	}
	else
	{
		div2.style.width="250px"; 	
	}
}
function changeState()
{
	var div6=document.getElementById('div6');
	if(parseInt(div6.style.width)==0||div6.style.width==0)
	{
		div6.style.width="100%";
		div6.style.height="100vh";
	}
}
function noChange()
{
	var div6=document.getElementById('div6');
	div6.style.width=0;
	div6.style.height=0;
}
function backAdd()
{
	var div6=document.getElementById('divx6');
	div6.style.width=0;
	div6.style.height=0;
}
function anim()
{
	if(event.target==document.getElementById('b3')||event.target==document.getElementById('b4'))
		return;
	document.getElementById("div7").style.animationPlayState = "running";
	setTimeout(function(){ document.getElementById("div7").style.animationPlayState = "paused"; }, 820);
}
function animman()
{
	if(event.target==document.getElementById('b3')||event.target==document.getElementById('b4'))
		return;
	document.getElementById("divman7").style.animationPlayState = "running";
	setTimeout(function(){ document.getElementById("divman7").style.animationPlayState = "paused"; }, 820);
}
function logOut()
{
	var div3=document.getElementById('div3');
	if(parseInt(div3.style.width)==0||div3.style.width==0)
	{
		div3.style.width="100%";
		div3.style.height="100vh";
	}
	else
	{
		var xhttp= new XMLHttpRequest();
		xhttp.open("GET","/logout");
		xhttp.send();
		xhttp.onload=function(){
			location.href="/";
		}
	}
}
function goHome()
{
	var div3=document.getElementById('div3');
	div3.style.width=0;
	div3.style.height=0;
}
function ani()
{
	if(event.target==document.getElementById('b2')||event.target==document.getElementById('b1'))
		return;
	document.getElementById("div4").style.animationPlayState = "running";
	setTimeout(function(){ document.getElementById("div4").style.animationPlayState = "paused"; }, 820);
}
function chState()
{
	location.href='/admin/switchAsAdmin';
}
</script>
