function logOut()
{
	var div3=document.getElementById('div3');
	if(parseInt(div3.style.width)==0||div3.style.width==0)
	{
		div3.style.width="100%";
		div3.style.height="100vh";
	}
	else
	{
		var xhttp= new XMLHttpRequest();
		xhttp.open("GET","/logout");
		xhttp.send();
		xhttp.onload=function(){
			location.href="/";
		}
	}
}
function goHome()
{
	var div3=document.getElementById('div3');
	div3.style.width=0;
	div3.style.height=0;
}
function chState()
{
	location.href='/admin/switchAsUser';
}
function ani()
{
	if(event.target==document.getElementById('b2')||event.target==document.getElementById('b1'))
		return;
	document.getElementById("div4").style.animationPlayState = "running";
	setTimeout(function(){ document.getElementById("div4").style.animationPlayState = "paused"; }, 820);
}
var flag;
if($(window).innerWidth()<=767)
	flag=0;
else
	flag=1;
$(window).resize(()=>{
	console.log(flag);
	if($(window).innerWidth()<=767&&flag==1)
	{
		if($("#pmain").position().left!=0)
			$("#pmain").animate({left: "0px",}, 400 );
		if(document.getElementById("div2").style.width=="250px")
		{
			document.getElementById('div2-wrapper').classList.toggle("hidden-xs");
			document.getElementById('div2-wrapper').classList.toggle("col-xs-10");
		}
		flag=0;
	}
	else if($(window).innerWidth()>767&&flag==0)
	{
		if(document.getElementById("div2").style.width=="250px")
			$("#pmain").animate({left: "200px",}, 400 );
		flag=1;
	}
});
</script>
