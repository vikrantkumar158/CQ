<script>
function funcNav()
{
	var div2=document.getElementById('div2');
	if($(window).width()<768)
	{
		document.getElementById('div2-wrapper').classList.toggle("hidden-xs");
		document.getElementById('div2-wrapper').classList.toggle("col-xs-10");
		if(div2.style.width=="250px")
		{
			div2.style.width="50px";
		}
		else
		{
			div2.style.width="250px"; 	
		} 	
	}
	else
	{
		if(div2.style.width=="250px")
		{
			div2.style.width="50px";
		}
		else
		{
			div2.style.width="250px"; 	
		}
	}
}
function changeState()
{
	var div6=document.getElementById('div6');
	if(parseInt(div6.style.width)==0||div6.style.width==0)
	{
		div6.style.width="100%";
		div6.style.height="100vh";
	}
}
function noChangeDe()
{
	var div6=document.getElementById('divde6');
	div6.style.width=0;
	div6.style.height=0;
}
function noChangeAc()
{
	var div6=document.getElementById('divac6');
	div6.style.width=0;
	div6.style.height=0;
}
function noChange()
{
	var div6=document.getElementById('div6');
